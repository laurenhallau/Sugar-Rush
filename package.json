  "bugs": {
    "url": "https://github.com/laurenhallau/Sugar-Rush/issues"
  },
  "homepage": "https://github.com/laurenhallau/Sugar-Rush#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^5.9.13"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "materialize-css": "^1.0.0",
    "mongoose": "^5.9.13",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-materialize": "^3.9.0",
    "react-router-dom": "^5.2.0"
  }
}
